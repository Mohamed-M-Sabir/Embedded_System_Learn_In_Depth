#include <stdio.h>
#include <stdlib.h>

int main(void) {


	puts("!!!Hello World!!!"); /* prints !!!Hello World!!! */
	return EXIT_SUCCESS;
}



int count_ones(int number)
{

	int counter=0,ones=0;

	while((1<<counter)<number){

		if(number & (1<<counter) > 0)
		{
			ones++;
		}
		counter++;
	}


	return counter;
}

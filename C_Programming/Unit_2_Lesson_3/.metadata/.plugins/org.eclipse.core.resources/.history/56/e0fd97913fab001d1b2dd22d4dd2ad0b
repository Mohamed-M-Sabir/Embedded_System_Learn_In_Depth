#include <stdio.h>
int main() {
	int num,fact=1;
	int i;
	printf("Enter an integer: ");
	fflush(stdout);
	scanf("%d", &num);
	fflush(stdin);

	for(i=0;i<=num;i++){

		fact*=i;
	}

	printf("fact =%d", fact);



	return 0;
}

#include<stdio.h>

int main(){

  int a,b,temp;

  printf("Enter two integers: ");
  fflush(stdout);
  scanf("%d" "%d",&a , &b);
  fflush(stdin);
  temp=a;
  a=b;
  b=temp;
  printf("\nAfter swapping value of a = %d\nAfter swapping value of b = %d",a,b);

  return 0;
}
